<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <RootNamespace>Wail_Vincent_AventCalender</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
	<ItemGroup>
		<None Include="Ressources\BGM\bgm1.wav">
			<CopyToOutputDirectory>preserveNewest</CopyToOutputDirectory>
		</None>
		<None Include="Ressources\BGM\bgm2.wav">
			<CopyToOutputDirectory>preserveNewest</CopyToOutputDirectory>
		</None>
	  <Resource Include="Ressources\Images\*.png" />
	</ItemGroup>
</Project>
